{
  "name": "stately-docs",
  "version": "0.1.0",
  "repository": "https://github.com/statelyai/docs",
  "scripts": {
    "docusaurus": "docusaurus",
    "remark:once": "yarn remark --quiet --frail docs/**/* versioned_docs/**/*",
    "remark:watch": "yarn remark --quiet --frail --watch docs/**/* versioned_docs/**/*",
    "start": "concurrently \"yarn remark:watch\" \"yarn typecheck:watch\" \"docusaurus start\"",
    "check-links": "yarn remark:once && yarn typecheck",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "remarkConfig": {
    "plugins": [
      [
        "remark-validate-links",
        {
          "repository": false
        }
      ]
    ]
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.3",
    "@docusaurus/plugin-client-redirects": "^2.4.3",
    "@docusaurus/preset-classic": "^2.4.3",
    "@docusaurus/remark-plugin-npm2yarn": "^2.4.3",
    "@mdx-js/react": "^1.6.22",
    "@microsoft/api-extractor": "^7.35.1",
    "@xstate/inspect": "^0.8.0",
    "@xstate/machine-extractor": "^0.9.2",
    "@xstate/react": "^3.2.2",
    "clsx": "^1.2.1",
    "docusaurus-plugin-api-extractor": "^2.0.4",
    "lucide-react": "^0.129.0",
    "mdx-embed": "^1.1.2",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typecheck": "^0.1.2",
    "xstate": "^4.37.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.4.3",
    "@fec/remark-a11y-emoji": "^3.1.0",
    "@tsconfig/docusaurus": "^1.0.7",
    "concurrently": "^8.2.1",
    "dotenv": "^16.0.3",
    "remark-cli": "^12.0.0",
    "remark-validate-links": "^13.0.0",
    "typescript": "^5.0.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
