{
  "name": "stately-docs",
  "version": "0.1.0",
  "repository": "https://github.com/statelyai/docs",
  "scripts": {
    "docusaurus": "docusaurus",
    "remark:once": "yarn remark --quiet --frail docs/**/* versioned_docs/**/*",
    "remark:watch": "yarn remark --quiet --frail --watch docs/**/* versioned_docs/**/*",
    "dev": "docusaurus start",
    "start": "concurrently \"yarn remark:watch\" \"yarn typecheck:watch\" \"docusaurus start\"",
    "check-links": "yarn remark:once && yarn typecheck",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,md,mdx,json}'"
  },
  "remarkConfig": {
    "plugins": [
      [
        "remark-validate-links",
        {
          "repository": false
        }
      ]
    ]
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.0",
    "@docusaurus/plugin-client-redirects": "^3.0.0",
    "@docusaurus/preset-classic": "^3.0.0",
    "@docusaurus/remark-plugin-npm2yarn": "^3.0.0",
    "@headlessui/react": "^1.7.17",
    "@mdx-js/react": "^3.0.0",
    "@microsoft/api-extractor": "^7.38.3",
    "@xstate/inspect": "^0.8.0",
    "@xstate/machine-extractor": "^0.13.0",
    "@xstate/react": "^3.2.2",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "docusaurus-plugin-api-extractor": "^2.0.4",
    "headlessui": "^0.0.0",
    "lucide-react": "^0.293.0",
    "mdx-embed": "^1.1.2",
    "postcss": "^8.4.31",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "tailwindcss-scoped-preflight": "^2.0.2",
    "typecheck": "^0.1.2",
    "xstate": "^4.38.3"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.0.0",
    "@docusaurus/tsconfig": "^3.0.0",
    "@docusaurus/types": "^3.0.0",
    "@fec/remark-a11y-emoji": "^4.0.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "remark-cli": "^12.0.0",
    "remark-validate-links": "^13.0.0",
    "typescript": "^5.3.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
